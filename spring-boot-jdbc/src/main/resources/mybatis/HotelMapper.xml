<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.spring.jdbc.mapper.HotelMapper">
    <select id="selectByCityId" resultType="com.example.spring.jdbc.model.Hotel">
        select * from hotel where city = #{id}
    </select>
</mapper>